<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">



    <!--同类抽象bean-->
    <bean id="student1" class="learn.spring.di.di08.Student" parent= "baseStudent">
       <property name="name" value="Tom" />
       <property name="age"  value="18" />
   </bean>

    <bean id="student2" class="learn.spring.di.di08.Student" parent="baseStudent">
        <property name="name" value="Tom2" />
        <property name="age"  value="19" />
    </bean>
    <bean id="student3" class="learn.spring.di.di08.Student" parent="baseStudent">
        <property name="name" value="Tom3" />
        <property name="age"  value="34" />
    </bean>
</beans>